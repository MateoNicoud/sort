function left(pivot) void{
    let i = pivot -1;
    while i < csvData.length{
      if isLess(i,pivot){
        i--;
      }
      else{
        swap(i,pivot);
        let moveLeft = true;
      }
    }
}

function right(pivot) void{
    let i = pivot +1;
    while i < csvData.length{
      if isLess(i,pivot){
        i++;
      }
      else{
        swap(i,pivot);
        let moveRight = true;
      }
    }
}